
<!DOCTYPE html>
<html>
<head>
    <title>Fluent Fusion</title>
    <link rel="website icon" href="https://encrypted-tbn1.gstatic.com/images?q=tbn:ANd9GcSI0pFMl9W_MoWl-sjRNRKrg_jGjHPMM6NJku0jsutPowFO7UEL">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Trirong">
    <style>
        .hide {
            display: none;
        }
        .activity img {
            cursor: pointer;
            width: 300px;
            height: 300px;
        }
        .correct {
            position: relative;
            padding: .75rem 1.25rem;
            margin: 1rem;
            border: 1px solid transparent;
            border-radius: .25rem;
            color: #155724;
            background-color: #d4edda;
            border-color: #c3e6cb;
            width: 80%;
        }
        .incorrect {
            position: relative;
            padding: .75rem 1.25rem;
            margin: 1rem;
            border: 1px solid transparent;
            border-radius: .25rem;
            color: #721c24;
            background-color: #f8d7da;
            border-color: #f5c6cb;
            width: 80%;
        }
        .item {
            position: relative;
            padding: .75rem 1.25rem;
            margin: 1rem;
            border: 1px solid transparent;
            border-radius: .25rem;
            color: #111;
            background-color: #efefef;
            border-color: #dfdfdf;
            width: 80%;
        }
        .next {
            background-color: rgb(27, 200, 36);
            color: #fff;
            border-radius: 5px;
            width: 300px;
            height: 50px;
            margin-top: 70px;
        }
        .previous {
            background-color: rgb(27, 200, 36);
            color: #fff;
            border-radius: 5px;
            width: 300px;
            height: 50px;
            margin-top: 70px;
        }
        .reset {
            background-color: rgb(27, 200, 36);
            color: #fff;
            border-radius: 5px;
            width: 300px;
            height: 50px;
            margin-top: 70px;
        }
        .question {
            color: black;
            letter-spacing: 1px;
            font-size: 34px;
        }
        .container {
            width: 90%;
            height: 300px;
            border: 1px solid black;
            border-radius: 50px;
            position: relative;
        }
        #section-1 {
            height: 350px;
        }
        #section-2 {
            height: 700px;
        }
        .welcome {
            color: black;
            position: absolute;
            top: 0;
            left: 30px;
            font-size: 40px;
            font-weight: bold;
            letter-spacing: 1px;
        }
        .description{
            color: #736868;
            position: absolute;
            top: 100px;
            left: 30px;
            font-size: 22px;
            font-weight: bold;
            letter-spacing: 1px;
            font-family: "Trirong", serif;
            

        }
        #score {
            font-size: 24px;
            font-weight: bold;
            color: #333;
            margin-top: 25px;
        }
        .activity div {
            text-align: center;
            font-size: 24px;
            margin-top: 5px;
            color: #333;
        }
        .image-label-set1 {
            text-align: center;
            font-size: 24px;
            margin-top: 5px;
            color: #333;
            display: block; /* Initially set to 'block' */
        }
        .image-label-set2 {
            text-align: center;
            font-size: 24px;
            margin-top: 5px;
            color: #333;
            display: none; /* Initially set to 'none' */
        }
        audio{
            display: none;
        }
    </style>
</head>
<body>
    <center>
        <section id="section-1">
            <div class="container">
                <h1 class="welcome">Welcome <%= session.username %></h1>
                <p class="description">Test your French vocabulary in this interactive quiz! Click the images to match the translations and boost your language skills. Bonne chance!</p>
            </div>
        </section>
    </center>
    <section id="section-2">
        <center>
            <h2>
                Please select the correct answer.
            </h2>
            <div id="question-box" class="item clearfix">
                <span id="questiontext" class="question"></span>
            </div>
            <table class="table activity" style="width:80%">
                <tr>
                    <td class="td1">
                        <img id="image001" name="Chat" src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcStB_mba6CybLznGBCsAa2IxqEE1MvgyVtC3Z6IQ4q5E0m3qMWZ_PoyEKXFC21nXEoyMAg&usqp=CAU" onclick="playChatSound()">
                        <div class="image-label image-label-set1" id="image-label1">Le Chat</div>
                    </td>
                    <td class="td2">
                        <img id="image002" name="Le garçon" src="https://img.freepik.com/free-vector/cute-boy-standing-position-showing-thumb_96037-450.jpg" onclick="playBoySound()">
                        <div class="image-label image-label-set1" id="image-label2">Le garçon</div>
                    </td>
                    <td class="td3">
                        <img id='image003' name="homme" src="https://thumbs.dreamstime.com/b/d-illustration-smiling-happy-man-tablet-cartoon-close-up-portrait-standing-businessman-isolated-white-d-illustration-199646648.jpg" onclick="playManSound()">
                        <div class="image-label image-label-set1" id="image-label3">homme</div>
                    </td>
                </tr>
                <tr>
                    <td class="td1">
                        <img id="image004" name="Fille" src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSh7YG1TMpan7W4sxo2QKBkOmDmBhv3NqR1oQ&usqp=CAU" onclick="playGirlSound()">
                        <div class="image-label image-label-set2" id="image-label4">Fille</div>
                    </td>
                    <td class="td2">
                        <img id="image005" name="chien" src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTPBzAyVPCoOgS88uIJCk4nPhAQIgbjIgMNKQ&usqp=CAU" onclick="playDogSound()">
                        <div class="image-label image-label-set2" id="image-label5">chien</div>
                    </td>
                    <td class="td3">
                        <img id="image006" name="un" src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQE9Y_Qap8Zr1-zANGieARPNnEKw1Uk2XjLJg&usqp=CAU" onclick="playOneSound()">
                        <div class="image-label image-label-set2" id="image-label6">un</div>
                    </td>
                </tr>
            </table>

            <audio controls id="audio1">
                <source src="https://drive.google.com/uc?id=1lOzpeHAkGecFonrwNHmxWK5nLIpRRRxo" type="audio/mpeg">
              </audio>
              <audio controls id="audio2">
                <source src="https://drive.google.com/uc?id=1FABWag5fkDRf30fCCANaPYu_RTLE7Loj" type="audio/mpeg">
              </audio>
              <audio controls id="audio3">
                <source src="https://drive.google.com/uc?id=101DMzAJ2a5VY4BFr9VKzMM0ZwLVpQ5Hl" type="audio/mpeg">
              </audio>

              <audio controls id="audio4">
                <source src="https://drive.google.com/uc?id=1Lxx2Bk5LWQtCZXoYWutK6nAINt35djcx" type="audio/mpeg"> 
              </audio>
              <audio controls id="audio5">
                <source src="https://drive.google.com/uc?id=13p2De4ufI2iCcSYebCGUInIpo5iKUsbJ" type="audio/mpeg">
              </audio>
              <audio controls id="audio6">
                <source src="https://drive.google.com/uc?id=1Z2eTccuRDrMwPSkdgcalJ5W0OysZ17F_" type="audio/mpeg">
              </audio>

            <div id="questionresponse" class="hide"></div>
            <button id="previousquestion" class="previous">
                Previous Question
            </button>
            <button id="nextquestion" class="next">
                Next Question
            </button>
            <button id="reset" class="reset">
                Re-start
            </button>
            <button id="submit"class="next">
                Submit
            </button>
            <div id="score">Score: 0 </div>
        </center>
    </section>
    <!-- <iframe id="dashboard1Frame" src="dashboard1.ejs" style="display: none;"></iframe> -->
    

    <script>
        var currentquestion = 0;

        function updateScore() {
            document.getElementById('score').textContent = 'Score: ' + score;
        }

        var questionsSet1 = [
        {
            "question": "Which one of these is 'the boy'?",
            "response": " 'Le garçon' to mean 'the boy' in French.",
            "answerimage": 'image002',
            "correctAnswer": "image002" 
        },
        {
            "question": "Which one of these is 'the cat'? ",
            "response": " 'Le Chat' to mean 'the cat' in French. ",
            "answerimage": 'image001',
            "correctAnswer": "image001"
        },
        {
            "question": "Which one of these is 'the man'?",
            "response": " 'homme' to mean 'the man' in French.",
            "answerimage": 'image003',
            "correctAnswer": "image003"
        }
        ];

        var questionsSet2 = [
            {
            "question": "Which one of these is 'the girl'?",
            "response": " 'Fille' to mean 'the girl' in French ",
            "answerimage": 'image004',
            "correctAnswer": "image004"
        },
        {
            "question": "Which one of these is 'the dog'?",
            "response": " 'chien' to mean 'the dog' in French  ",
            "answerimage": 'image005',
            "correctAnswer": "image005"
        },
        {
            "question": "Which one of these is 'One'?",
            "response": " 'un' to mean 'one' in French ",
            "answerimage": 'image006',
            "correctAnswer": "image006"
        }
        ];

        var currentQuestions = questionsSet1;
        const images123 = [
        "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcStB_mba6CybLznGBCsAa2IxqEE1MvgyVtC3Z6IQ4q5E0m3qMWZ_PoyEKXFC21nXEoyMAg&usqp=CAU", 
        "https://img.freepik.com/free-vector/cute-boy-standing-position-showing-thumb_96037-450.jpg", 
        "https://thumbs.dreamstime.com/b/d-illustration-smiling-happy-man-tablet-cartoon-close-up-portrait-standing-businessman-isolated-white-d-illustration-199646648.jpg" 
    ];

  
    const images456 = [
        "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSh7YG1TMpan7W4sxo2QKBkOmDmBhv3NqR1oQ&usqp=CAU", 
        "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTPBzAyVPCoOgS88uIJCk4nPhAQIgbjIgMNKQ&usqp=CAU", 
        "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQE9Y_Qap8Zr1-zANGieARPNnEKw1Uk2XjLJg&usqp=CAU" 
    ];

        var labelsSet1 = ["Chat", "Le garçon", "homme"];
        var labelsSet2 = ["Fille", "chien", "un"];

        var score = 0;

        updateImagesForQuestion(currentquestion);

        (function () {
            updateButtonVisibility();
        })();

        function updateImagesForQuestion(questionNumber) {
            if (currentQuestions === questionsSet1) {
                for (var i = 1; i <= 6; i++) {
                    document.getElementById('image00' + i).style.display = i <= 3 ? 'block' : 'none';
                    document.getElementById('image-label' + i).style.display = i <= 3 ? 'block' : 'none';
                }

                for (var i = 1; i <= 3; i++) {
                    document.getElementById('image-label' + i).textContent = labelsSet1[i - 1];
                }
            } else {
                for (var i = 1; i <= 6; i++) {
                    document.getElementById('image00' + i).style.display = i > 3 ? 'block' : 'none';
                    document.getElementById('image-label' + i).style.display = i > 3 ? 'block' : 'none';
                }

                for (var i = 4; i <= 6; i++) {
                    document.getElementById('image-label' + i).textContent = labelsSet2[i - 4];
                }
            }

            document.getElementById('questiontext').innerHTML = currentQuestions[questionNumber].question;
        }

        (function () {
            fnReset();
            setupEventListeners();
        })();

        function fnReset() {
            document.getElementById('questionresponse').classList.add('hide');
            currentQuestions = questionsSet1;
            currentquestion = 0;
            score = 0;
            updateImagesForQuestion(currentquestion);
            updateScore();
            document.getElementById('questiontext').innerHTML = currentQuestions[currentquestion].question;
            updateButtonVisibility();
        }

        function fnNext() {
            document.getElementById('questionresponse').classList.add('hide');
            if (currentquestion < currentQuestions.length - 1) {
                currentquestion = currentquestion + 1;
            } else {
                if (currentQuestions === questionsSet1) {
                    currentQuestions = questionsSet2;
                    currentquestion = 0;
                    updateImagesForQuestion(currentquestion);
                }else if(currentQuestions === questionsSet2 && currentquestion === currentQuestions.length - 1){
                    isSubmitVisible = true;
                }
            }
            document.getElementById('questiontext').innerHTML = currentQuestions[currentquestion].question;
            updateButtonVisibility();
        }
    

        function fnPrevious() {
            document.getElementById('questionresponse').classList.add('hide');
            if (currentquestion > 0) {
                currentquestion = currentquestion - 1;
            } else if (currentQuestions === questionsSet2) {
                currentQuestions = questionsSet1;
                currentquestion = currentQuestions.length - 1;
                updateImagesForQuestion(currentquestion);
            }
            document.getElementById('questiontext').innerHTML = currentQuestions[currentquestion].question;
            updateButtonVisibility();
        }
        var isSubmitVisible = false;

        function updateButtonVisibility() {
            if (currentQuestions === questionsSet1 && (currentquestion === 1 || currentquestion === 2)) {
                document.getElementById('previousquestion').classList.remove('hide');
            } else if (currentQuestions === questionsSet2 && (currentquestion === 0 || currentquestion === 1 || currentquestion === 2)) {
                document.getElementById('previousquestion').classList.remove('hide');
            } else {
                document.getElementById('previousquestion').classList.add('hide');
            }

            if (currentquestion === currentQuestions.length - 1 && currentQuestions === questionsSet2) {
                if(isSubmitVisible){
                document.getElementById('nextquestion').classList.add('hide');
                document.getElementById('reset').classList.remove('hide');
                document.getElementById('submit').classList.remove('hide');
                }
                else{
                document.getElementById('nextquestion').classList.remove('hide');
                document.getElementById('reset').classList.add('hide');
                document.getElementById('submit').classList.add('hide');
                }
            } else {
                document.getElementById('nextquestion').classList.remove('hide');
                document.getElementById('reset').classList.add('hide');
                document.getElementById('submit').classList.add('hide');
            }
            
            
        }
        
        var score=0

        function setupEventListeners() {
            document.getElementById("nextquestion").addEventListener('click', function (event) {
                fnNext();
                updateButtonVisibility();
            });

            document.getElementById("previousquestion").addEventListener('click', function (event) {
                fnPrevious();
                updateButtonVisibility();
            });

            document.getElementById("reset").addEventListener('click', function (event) {
                fnReset();
                updateButtonVisibility();
            });
             document.getElementById("submit").addEventListener('click', function (event) {
                  if (score >= 6) {
                    const imageUrl = "data:image/png;base64,";
                    window.location.href = 'dashboard2?imageUrl=${encodeURIComponent(imageUrl)}';
                     window.location.href="dashboard2"
                 } else {
                 alert("You need to score 6 to proceed.");         
         }
         });


            // Add your image click event listeners here
            document.getElementById("image001").addEventListener('click', function (event) {
                fnCheck("image001", this);
            });

            document.getElementById("image002").addEventListener('click', function (event) {
                fnCheck("image002", this);
            });

            document.getElementById("image003").addEventListener('click', function (event) {
                fnCheck("image003", this);
            });

            document.getElementById("image004").addEventListener('click', function (event) {
                fnCheck("image004", this);
            });

            document.getElementById("image005").addEventListener('click', function (event) {
                fnCheck("image005", this);
            });

            document.getElementById("image006").addEventListener('click', function (event) {
                fnCheck("image006", this);
            });

            document.getElementById("reset").addEventListener('click', function (event) {
                fnReset();
            });
        }

        function fnCheck(answerimage, obj) {
            document.getElementById('questionresponse').classList.remove('correct');
            document.getElementById('questionresponse').classList.remove('incorrect');
            if (currentQuestions[currentquestion].correctAnswer === answerimage) {
                document.getElementById('questionresponse').classList.add('correct');
                document.getElementById('questionresponse').innerHTML = "Correct: The answer is '" + obj.name + "'. " + currentQuestions[currentquestion].response;
                document.getElementById('nextquestion').classList.remove('hide');
                score++;
                updateScore();
        //         if (score === currentQuestions.length) {
         
                
        //         window.location.href = 'dashboard1?score=' + score;
        //  }
            } else {
                document.getElementById('questionresponse').classList.add('incorrect');
                document.getElementById('questionresponse').innerHTML = "Incorrect: You chose: '" + obj.name + "'. Please try again.";
            }
            document.getElementById('questionresponse').classList.remove('hide');
        }

        function playChatSound() {
        var audio = document.getElementById("audio1");
        audio.style.display = "none"; 
        audio.play();
      }

      function playBoySound() {
        var audio = document.getElementById("audio2");
        audio.style.display = "none"; 
        audio.play();
      }

      function playManSound() {
        var audio = document.getElementById("audio3");
        audio.style.display = "none"; 
        audio.play();
      }
 
      function playGirlSound() {
        var audio = document.getElementById("audio4");
        audio.style.display = "none"; 
        audio.play();
      }

      function playDogSound() {
        var audio = document.getElementById("audio5");
        audio.style.display = "none"; 
        audio.play();
      }

      function playOneSound() {
        var audio = document.getElementById("audio6");
        audio.style.display = "none"; 
        audio.play();
      }
    //   function changeTaskImage(imageSrc) {
    //         // This function will call the changeTaskImage function inside the iframe
    //         var dashboard1Frame = document.getElementById('dashboard1Frame');
    //         if (dashboard1Frame) {
    //             dashboard1Frame.contentWindow.changeTaskImage(imageSrc);
    //         }
    //     }
    document.getElementById("submit").addEventListener('click', function (event) {
        if (score >= 6) {
                const imageUrl = "data:image/png;base64,";  // Replace with your actual image URL
                window.location.href = `dashboard2?imageUrl=${encodeURIComponent(imageUrl)}`;
            } else {
                alert("You need to score 6 to proceed.");
            }
        });

    </script>
    
    </body>
</html>