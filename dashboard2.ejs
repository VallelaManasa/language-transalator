    <script>
       function openNav() {
            document.getElementById("mySidenav").style.width = "250px";
        }

        function closeNav() {
            document.getElementById("mySidenav").style.width = "0";
        }

        function selectMenuItem(menuItem) {
            const menuItems = document.querySelectorAll('.sidenav li');
            menuItems.forEach(item => item.classList.remove('selected'));
            const selectedMenuItem = document.querySelector(a[onclick*='${menuItem}']);
            selectedMenuItem.closest('li').classList.add('selected');
        }
        function getUrlParameter(name) {
            name = name.replace(/[\[]/, '\\[').replace(/[\]]/, '\\]');
            const regex = new RegExp('[\\?&]' + name + '=([^&#]*)');
            const results = regex.exec(location.search);
            return results === null ? '' : decodeURIComponent(results[1].replace(/\+/g, ' '));
        }

        // Get the image URL parameter from the URL
        const imageUrlParam = getUrlParameter('imageUrl');

        // Update the task2 image source
        const task2Image = document.getElementById('task2-image');
        if (imageUrlParam) {
            task2Image.src = imageUrlParam;
        }

        function toggleGuideBookContent() {
            const sections = document.querySelectorAll('.content section');
            const guideBookContent = document.getElementById("guideBookContent");

            if (guideBookContent.style.display === "none" || guideBookContent.style.display === "") {
                sections.forEach((section) => {
                    section.style.display = "none";
                });
                guideBookContent.style.display = "block";
                } else {
                    sections.forEach((section) => {
                    section.style.display = "block";
                });
                guideBookContent.style.display = "none";
            }
        }
        function speakText(text, lang) {
            const utterance = new SpeechSynthesisUtterance(text);
            utterance.lang = lang;
             speechSynthesis.speak(utterance);
        }
        const fromVolumeIcon = document.querySelector(".row.from .icons .fa-volume-up");


        fromVolumeIcon.addEventListener("click", () => {
            
        const fromText = document.querySelector(".from-text").value;

  
         const fromLanguage = document.querySelector(".row.from select").value;

  
        speakText(fromText, fromLanguage);
        });
        
