    <div class="error-msg" id="error-msg"></div>
    <div class="button-container">
        <button id="previous-set">Previous</button>
        <button id="next-set">Next</button>
        <button id="reset-set">Reset</button>
        <button id="submit" >Submit</button>
    </div>
    <div class="score">Score: <span id="score">0</span> &#10084;</div>

    <script>
        const options = Array.from(document.querySelectorAll('.option'));
        options.forEach(option => {
        option.addEventListener('mouseover', () => {
            const word = option.textContent;
            responsiveVoice.speak(word, 'French Female', { rate: 1 });
        });
    });
        
        let selectedOptions = [];
        let score = 0;
        const errorMsg = document.getElementById('error-msg');
        const scoreElement = document.getElementById('score');
        const matchingSets = document.querySelectorAll('.matching-set');
        let currentSetIndex = 0;
       function showMatchingSet(index) {
            matchingSets.forEach((set, i) => {
                if (i === index) {
                    set.classList.add('active');
                } else {
                    set.classList.remove('active');
                }
            });
        }

        showMatchingSet(currentSetIndex);

        const previousSetButton = document.getElementById('previous-set');
    const nextSetButton = document.getElementById('next-set');
    const resetSetButton = document.getElementById('reset-set');

    const submitButton = document.getElementById('submit-answers');

    previousSetButton.addEventListener('click', () => {
        currentSetIndex = (currentSetIndex - 1 + matchingSets.length) % matchingSets.length;
        showMatchingSet(currentSetIndex);
        resetScoreAndSelections();
    });

    nextSetButton.addEventListener('click', () => {
        currentSetIndex = (currentSetIndex + 1) % matchingSets.length;
        showMatchingSet(currentSetIndex);
        resetScoreAndSelections();
    });

    resetSetButton.addEventListener('click', () => {
        currentSetIndex = 0;
        showMatchingSet(currentSetIndex);
        resetScoreAndSelections();
    });
    document.getElementById("submit").addEventListener('click', function (event) {
        if (score === 5) {
                const imageUrl = "data:image/png;base64,";  // Replace with your actual image URL
                window.location.href = `dashboard2?imageUrl=${encodeURIComponent(imageUrl)}`;
            } else {
                alert("You need to score 5 to proceed.");
            }
        });



        function checkIfAllSetsCompleted() {
    const submitButton = document.getElementById('submit');
    if (currentSetIndex === 4) {  // Check if it's the 5th set
        submitButton.style.display = 'block';
    } else {
        submitButton.style.display = 'none';
    }
}

function showMatchingSet(index) {
    matchingSets.forEach((set, i) => {
        if (i === index) {
            set.classList.add('active');
        } else {
            set.classList.remove('active');
        }
    });
    checkIfAllSetsCompleted(); // Add this line to update the submit button visibility
}






        options.forEach(option => {
            option.addEventListener('click', () => {
                if (!option.classList.contains('matched') && selectedOptions.length < 2) {
                    option.classList.add('selected');
                    selectedOptions.push(option);

                    if (selectedOptions.length === 2) {
                        const [option1, option2] = selectedOptions;

                        if (option1.getAttribute('data-match') === option2.getAttribute('data-match')) {
                            option1.classList.add('correct', 'matched');
                            option2.classList.add('correct', 'matched');
                            score++;
                            scoreElement.textContent = score;
                            errorMsg.style.backgroundColor = 'rgb(27, 200, 36)';
                            errorMsg.textContent = 'correct match!';
                            // errorMsg.style.backgroundColor = 'greenyellow';
                        } else {
                            errorMsg.textContent = 'Incorrect match! Try again.';
                            errorMsg.style.backgroundColor = '#f8d7da';
                            setTimeout(() => {
                                option1.classList.remove('selected');
                                option2.classList.remove('selected');
                                // errorMsg.style.backgroundColor = '#f8d7da';
                            }, 1000);
                        }

                        selectedOptions = [];
                    }
                }
                checkIfAllSetsCompleted();
            });
        });

        function resetScoreAndSelections() {
            options.forEach(option => {
                option.classList.remove('selected', 'correct', 'matched');
            });

            selectedOptions = [];
            score = 0;
            scoreElement.textContent = score;
            errorMsg.style.backgroundColor = '';
            errorMsg.textContent = '';
        }
  

//         function submitAnswers() {

