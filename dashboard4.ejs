    <style>
        body {
            font-family: Arial, sans-serif;
            background-color: #f2f2f2;
            margin: 0;
            padding: 0;
        }

        .quiz-container {
            
            margin-left: 5%; 
             align-items: center;
            justify-content: center;
            
            
        }

        .column1 {
            flex: 1;
            padding: 10px;
            margin-left: 300px;
          
        }

        .column2 {
            flex: 1;
            padding: 10px;
            margin-right: 350px;
            
        }

        .column1 h2 {
            text-align: center;
            margin-left: 60px;
            margin-top: 10px;
        }
        .column2 h2 {
            text-align: center;
            margin-right: 20px;
            margin-top: 10px;
        }

        .options {
            list-style-type: none;
            padding: 0;
            width: 150px;
            margin-left: 35%;
           
        }

        .option {
            margin: 5px 0;
            padding: 5px;
            border: 1px solid #ccc;
            cursor: pointer;
            box-shadow: rgba(211, 18, 18, 0.2);
            border-radius: 10%;
            font-size: 1.6rem;
            text-align: center;
            margin-right: 10px;
            
            
        }
        

        .option:hover {
            background-color:#ccc;
        }
       .button-container {
           text-align: center;
           margin: 20px 0;
        }

      .button-container button {
        margin: 5px;
        background-color: rgb(27, 200, 36);
        color: #fff;
        border: none;
        border-radius: 5px;
        width: 100px;
        height: 40px;
        font-size: 16px;
        cursor: pointer;
       }
        

        .score {
             text-align: center; 
            font-size: 24px;
            font-weight: bold;
            color: #333;
            margin-top:25px;

        }

        /* .error-msg {
            text-align: center;
            color: red;
        } */

        .matching-set {
            display: none;
        }

        .matching-set.active {
            display: flex;
            align-items: center;
        }
        .error-msg {
            position: relative;
            padding: .75rem 1.25rem;
            margin: 1rem;
            border: 1px solid transparent;
            border-radius: .25rem;
            color: #721c24;
            background-color:'';
            width:100%;
            text-align: center;    
        }
        #submit {
            text-align: center;
           margin: 20px 0;
           margin-left:720px;
           margin-top:0px;
        }

    </style>
</head>
<body>
   <div class="quiz-container matching-set active" id="matching-set-1">
